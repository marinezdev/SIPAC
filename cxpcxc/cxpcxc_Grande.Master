                            <b><u><asp:Label ID="lbMstEmpresa" runat="server" Font-Size="15px"  ></asp:Label></u></b>
                        </td>
                        <td style ="text-align:right">
                            <img id ="ImgLogo" src="img/logo_asae.png" style="height: 115px; width: 225px" runat ="server"   />
                        </td>
                    </tr>
                </table>
            </div>
            <div style=" text-align :right;background-color:#D7E4F2; width :100%;height:30px;"">
                <b><asp:Label ID="lbMstNombreUsuario" runat="server" ForeColor="#102167" Font-Size="12px"></asp:Label></b> 
            </div>
            <div id="dvCajaAreaTrabajo"  margin: auto;">
                <asp:ContentPlaceHolder ID="cphAreaTrabajo" runat="server"></asp:ContentPlaceHolder>
